{
  "proto": {
    "id" : "?genre",
    "artist" : {
      "id": "?band",
      "album": {
        "id": "?album",
        "label": "$dbo:recordLabel$required",
        "date": "$dbo:releaseDate"
      }
    }

  },
  "$where": [
    "?band a dbo:Band",
    "?album a schema:MusicAlbum",
    "?id dbo:genre ?genre",
    "?album dbo:artist ?band"
  ],
  "$limit": 10000,
  "grlc": "#+ summary: Testing DBpedia endpoint\n#+ endpoint: http://dbpedia.org/sparql\n#+ tags:\n#+   - sparql\n#+ method: GET\n#+ pagination: 50",
  "$prefixes": {
    "dbo" : "http://dbpedia.org/ontology/"
  }
}
